                        },
                        "passcode": {
                            "title": "Passcode",
                            "type": "string",
                            "pattern": "\\d{5}"
                        },
                        "memorable_word": {
                            "title": "Memorable word",
                            "type": "string"
                        }
                    }
                },
                "ui_schema": {
                    "card_number": {
                        "ui:placeholder": "0000-0000-0000-0000"
                    },
                    "passcode": {
                        "ui:widget": "password"
                    },
                    "ui:order": [
                        "last_name",
                        "auth_method",
                        "passcode",
                        "memorable_word"
                    ]
                }
