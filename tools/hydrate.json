            "id": "barclays_uk",
            "description": "Barclays (UK)",
            "website_url": "https://www.barclays.co.uk/",
            "credentials_schema": {
                "json_schema": {
                    "$schema": "http://json-schema.org/draft-07/schema#",
                    "type": "object",
                    "required": [
                        "auth_method", 
                        "last_name", 
                        "passcode", 
                        "memorable_word"
                    ],
                    "properties": {
                        "last_name": {
                            "title": "Last name",
                            "type": "string"
                        },
