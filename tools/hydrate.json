    {
      "id": "interactive_brokers_uk",
      "description": "Interactive Brokers (UK)",
      "website_url": "https://www.interactivebrokers.com/en/home.php",
      "credentials_schema": {
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "required": [
            "report_file_pattern",
            "intake_method"
          ],
          "properties": {
            "report_file_pattern": {
              "type": "string",
              "title": "Report file pattern",
              "default": "*.xml"
            },
            "intake_method": {
              "type": "object",
              "title": "How would you like us to load your InteractiveBrokers Flex reports?",
              "oneOf": [
                {
                  "title": "From a local directory",
                  "required": [
                    "method_type",
                    "local_intake_dir"
                  ],
                  "properties": {
                    "method_type": {
                      "type": "string",
                      "default": "local_file"
                    },
                    "local_intake_dir": {
                      "title": "Local directory",
                      "type": "string"
                    }
                  }
                },
                {
                  "title": "From an FTP server",
                  "required": [
                    "method_type",
                    "ftp_host",
                    "username",
                    "password",
                    "directory",
                    "port"
                  ],
                  "properties": {
                    "method_type": {
                      "type": "string",
                      "default": "ftp"
                    },
                    "ftp_host": {
                      "title": "Host",
                      "type": "string",
                      "default": "ftp2.interactivebrokers.com"
                    },
                    "username": {
                      "title": "Username",
                      "type": "string"
                    },
                    "password": {
                      "title": "Password",
                      "type": "string"
                    },
                    "directory": {
                      "title": "Directory",
                      "type": "string",
                      "default": "outgoing"
                    },
                    "port": {
                      "title": "Port",
                      "type": "number",
                      "default": 21
                    }
                  }
                }
              ]
            },
            "pgp_key": {
              "type": "object",
              "title": "PGP key (used to decrypt report)",
              "required": [
                "data_base64"
              ],
              "properties": {
                "data_base64": {
                  "title": "PGP key (base64-encoded)",
                  "type": "string"
                },
                "passphrase": {
                  "title": "Passphrase",
                  "type": "string"
                }
              }
            }
          }
        },
        "ui_schema": {
          "intake_method": {
            "method_type": {
              "ui:widget": "hidden"
            },
            "password": {
              "ui:widget": "password"
            },
            "ui:order": [
              "method_type",
              "local_intake_dir",
              "ftp_host",
              "port",
              "username",
              "password",
              "directory"
            ]
          },
          "pgp_key": {
            "data_base64": {
              "ui:widget": "textarea"
            },
            "passphrase": {
              "ui:widget": "password"
            }
          },
          "ui:order": [
            "report_file_pattern",
            "intake_method",
            "pgp_key"
          ]
        }
      }
    },
